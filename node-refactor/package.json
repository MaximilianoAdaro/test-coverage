{
  "name": "refactor-check",
  "version": "1.0.0",
  "description": "Welcome to our technical challenge! We're excited to have you here and hope you're ready to showcase your skills.",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "test": "bash ./scripts/test-db.sh",
    "dev": "bash ./scripts/run-dev.sh",
    "prod": "bash ./scripts/run-prod.sh",
    "test:jest": "DOTENV_CONFIG_PATH=.env.jest jest -i --setupFiles dotenv/config --detectOpenHandles",
    "test:jest:dev": "DOTENV_CONFIG_PATH=.env.jest jest -i --coverage --setupFiles dotenv/config --detectOpenHandles",
    "start": "node src/index.js",
    "start:dev": "dotenv -e .env nodemon src/index.js",
    "start:prod": "dotenv -e .env.production node src/index.js",
    "db:drop": "dotenv -e .env npx sequelize-cli db:drop",
    "db:drop:test": "dotenv -e .env.jest npx sequelize-cli db:drop",
    "db:drop:prod": "dotenv -e .env.production npx sequelize-cli db:drop",
    "db:create": "dotenv -e .env npx sequelize-cli db:create",
    "db:create:test": "dotenv -e .env.jest npx sequelize-cli db:create",
    "db:create:prod": "dotenv -e .env.production npx sequelize-cli db:create",
    "db:migrate": "dotenv -e .env npx sequelize-cli db:migrate",
    "db:migrate:test": "dotenv -e .env.jest npx sequelize-cli db:migrate",
    "db:migrate:prod": "dotenv -e .env.production npx sequelize-cli db:migrate",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate",
    "db:reset:test": "npm run db:drop:test && npm run db:create:test && npm run db:migrate:test",
    "db:reset:prod": "npm run db:drop:prod && npm run db:create:prod && npm run db:migrate:prod",
    "db:seed": "dotenv -e .env npx sequelize-cli db:seed:all",
    "db:seed:prod": "dotenv -e .env.production npx sequelize-cli db:seed:all",
    "db:reset:seed": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "db:reset:seed:prod": "npm run db:reset:prod && npm run db:seed:prod",
    "db:seed:undo": "dotenv -e .env npx sequelize-cli db:seed:undo",
    "lint": "eslint --fix && prettier --write ."
  },
  "_moduleAliases": {
    "@src": "src",
    "@db": "src/db",
    "@utils": "src/utils",
    "@test": "__test__"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.38.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-json": "^3.1.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "2.8.7",
    "sequelize-cli": "^6.6.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "express": "^4.18.2",
    "fast-csv": "^4.3.6",
    "http-status": "^1.6.2",
    "joi": "^17.9.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "process": "^0.11.10",
    "sequelize": "^6.30.0",
    "signale": "^1.4.0",
    "xlsx": "^0.18.5",
    "yargs": "^17.7.1"
  }
}
